<div class="container-fluid">
	<div class="row">
		<div class="col-md-12 photo-container">
			<% @cities.each do |city| %>
				<%= link_to image_tag(city.image_url, :class => "cover-photo img-responsive", :id => "#{city.name}"), city_path(city) %>
			<% end %>
				<div class="arrows"> <span class="back-arrow"> &#9664</span><span class="forward-arrow">&#9654</span> </div>
		</div>
	</div>
</div>